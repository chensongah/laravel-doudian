{
    "name": "smartjson/laravel-doudian",
    "description": "抖音小店开放平台SDK",
    "keywords": [
        "smartjson",
        "doudian",
        "抖音小店",
        "抖店开放平台",
        "今日特卖",
        "抖店",
        "jinritemai"
    ],
    "homepage": "https://github.com/chensongah/laravel-doudian",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Smartjson",
            "email": "chensongah@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": ">=6.5",
        "illuminate/support": ">=6.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SmartJson\\DouDian\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartJson\\DouDian\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartJson\\DouDian\\DouDianServiceProvider"
            ],
            "aliases": {
                "DouDian": "SmartJson\\DouDian\\DouDianFacade"
            }
        }
    }
}
